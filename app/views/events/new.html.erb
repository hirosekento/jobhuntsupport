<%= form_with model: @event, url: { action: :create } do |form| %>

  <div class="title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="time">
    <%= form.label :start_time %>
    <%= form.datetime_select :start_time %>
  </div>

  <div class="content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>

  <div class="corp">
    <%= form.select 'corp_id',selectCorp(@corps), include_blank: true %>
  </div>

  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>